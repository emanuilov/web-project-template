{
	"extends": [
		"airbnb-base/legacy",
		"prettier"
	],
	"plugins": [
		"eslint-plugin-prettier"
	],
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"jest": true,
		"es6": true,
		"jquery": true
	},
	"rules": {
		"no-console": 1,
		"one-var": 0,
		"func-names": 0,
		"linebreak-style": 0,
		"class-methods-use-this": 0,
		"prettier/prettier": [
			"error",
			{
				"useTabs": true,
				"trailingComma": "none",
				"singleQuote": true,
				"printWidth": 100
			}
		]
	},
	"globals": {
		"__DEV__": true,
		"$": true
	}
}